programa {
  funcao inicio() {
  real comprimento,largura,area
  escreva ("digite o comprimento do terreno: ")
  leia (comprimento)
  escreva ("\n")
  escreva ("digite a largura do terreno: ")
  leia(largura)
  escreva ("\n")
  calcula ("a área do terreno")
